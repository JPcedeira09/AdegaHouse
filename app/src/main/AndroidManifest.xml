<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.guiay.adegahouse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".adapter.AdapterMostra"></activity>
        <activity android:name=".activity.MostraPedido" />
        <activity android:name=".activity.Slider" android:theme="@style/Theme.Intro" />
        <activity
            android:name=".activity.PopActivity"
            android:theme="@style/AppTheme.PopUP" />
        <activity
            android:name=".activity.menuDono"
            android:label="@string/title_activity_menu_dono"
            android:parentActivityName=".activity.PedidosEstoque"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.guiay.adegahouse.activity.PedidosEstoque" />
        </activity>
        <activity android:name=".adapter.AdapterPedidos" />
        <activity android:name=".activity.PedidosEstoque" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Login" />
        <activity android:name=".activity.Cadastro" />
        <activity android:name=".activity.PedidosCardapio" />
    </application>

</manifest>